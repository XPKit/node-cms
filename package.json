{
  "name": "node-cms",
  "version": "1.2.7",
  "description": "Content management system, written in node.js, uses leveldb as an asset and content store.",
  "main": "index.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build --emptyOutDir",
    "preview": "vite preview",
    "serve-backend": "cross-env VITE_DEV_MODE=true node server.js",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore --fix src",
    "test": "mocha --exit -R spec -b -t 40000 test/spec.js",
    "unit": "mocha -R spec -b -w -t 10000 test/unit",
    "mongodb": "ulimit -n 1000 && mongod --ipv6 --dbpath=../node-cms-mongodb-data",
    "mongodb-auth": "ulimit -n 1000 && mongod --tlsMode requireTLS --tlsCertificateKeyFile ./ssl/server.pem --tlsCAFile ./ssl/ca.pem --bind_ip localhost --dbpath=../node-cms-mongodb-data",
    "postinstall": "npm run build"
  },
  "engines": {
    "node": "^18"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:XPKit/node-cms.git"
  },
  "keywords": [
    "cms",
    "node.js",
    "express",
    "leveldb",
    "mongodb",
    "xpkit"
  ],
  "pkg": {
    "assets": [
      "src/**/*"
    ]
  },
  "bin": {
    "cms": "./server.js",
    "cms-import": "./import.js"
  },
  "author": "Kong Yim <kong.yim@imagination.com>",
  "license": "ISC",
